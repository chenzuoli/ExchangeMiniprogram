/* pages/moment-detail/index.wxss */
page{
  padding-top: 100rpx;
}
.pageContent{
  width: 100%;
  height: 100%;
}
.van-tabs__wrap{
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
}
.deltte{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 65px;
  height: 100%;
  background-color: red;
  color: #fff;
}
.scroll{
  height: calc(100vh - 100rpx);
}
.loading{
  display: flex;
  justify-content: center;
  min-height: 60rpx;
}